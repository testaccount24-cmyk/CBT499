)INIT
  .HELP = ORAHELP
)PROC
  VER(&UPROJ, NONBLANK)
  VER(&UPROJ, NAME)
  VER(&UMEM,  NONBLANK)
  VER(&UMEM,  NAME)
  VER(&UID,   NONBLANK)
  VER(&UPSW,  NONBLANK)
  VPUT (UPROJ UMEM) PROFILE
  &ZCMD = ' '
)END
./ ADD NAME=ORALOGIN 0101-01016-01016-1416-00038-00038-00000-RACFPNL
)ATTR
% type(text)  intens(high)
+ type(text)  intens(low)
$ type(input) intens(non)
_ type(input) intens(high)
)BODY
%--- &ZSYSID --------------- ORACLE LOGIN INFORMATION -------------- &ZSYSID ---
%OPTION ===>_ZCMD
+
+
+       LIBRARY DETAILS:
+
+           PROJECT ===> _UPROJ   +
+
+
+       ORACLE USER ACCOUNT DETAIL:
+
+           USERID   ===> _UID        +
+
+           PASSWORD ===> $UPSW       +
+
+           DATABASE ===> _UDB +
+
+
+
+   Enter %END+ command to terminate.
)INIT
  .HELP = ORAHELP
)PROC
  VER(&UPROJ, NONBLANK)
  VER(&UPROJ, NAME)
  VER(&UID,   NONBLANK)
  VER(&UPSW,  NONBLANK)
  VER(&UDB,   NONBLANK)
  VER(&UDB,   LEN, '>=', 3)
  VPUT (UPROJ UID UPSW UDB) PROFILE
  &ZCMD = ' '
)END
./ ADD NAME=ORAMAIN  0101-01016-01016-1416-00047-00047-00000-RACFPNL
)ATTR
% type(text)  intens(high)
+ type(text)  intens(low)
$ type(input) intens(non)
_ type(input) intens(high)
)BODY
%--- &ZSYSID ---------------- ORACLE UTILITIES MENU ---------------- &ZSYSID ---
%OPTION ===>_ZCMD
+
+
+   %S+  SQL       - SQL*PLUS
+   %F+  FORMS     - SQL*FORMS V2.3
+   %F3+ FORMS3    - SQL*FORMS V3.0
+   %V+  CVT       - CONVERT FORMS TO CICS READABLE FORMAT
+   %D+  DBA       - SQL*DBA
+   %O+  ORAMON    - ORACLE MONITOR
+   %P+  DELPHI    - DELPHI PERFORMANCE MONITOR
+
+
+           OOOOOOO  RRRRRRRR  AAAAAAAA  CCCCCCCC  LLL        EEEEEEEE
+         OOOOOOOOO RRRRRRRRR AAAAAAAAA CCCCCCCCC LLL        EEEEEEEE
+        OOO   OOO RRR   RRR AAA   AAA CCC       LLL        EEE
+       OOO   OOO RRRRRRRR  AAA   AAA CCC       LLL        EEE
+      OOO   OOO RRR  RRR  AAAAAAAAA CCC       LLL        EEEEEEEE
+     OOO   OOO RRR   RRR AAA   AAA CCC       LLL        EEE
+    OOOOOOOOO RRR   RRR AAA   AAA CCCCCCCCC LLLLLLLLL  EEEEEEEEE
+    OOOOOOO  RRR   RRR AAA   AAA CCCCCCCCC LLLLLLLLL  EEEEEEEEE
+
+
+   Enter %END+ command to terminate.
)INIT
  .HELP = ORAHELP
)PROC
  &ZCMD = TRUNC(&ZCMD, ' ')
  &ZSEL = TRANS(TRUNC(&ZCMD, '.')
        S, 'CMD(%SQL) MODE(FSCR)'
        F, 'CMD(%FORMS) MODE(FSCR)'
        F3, 'CMD(%FORMS3) MODE(FSCR)'
        V, 'CMD(%ORACVTM &UPROJ) NOCHECK'
        D, 'CMD(%DBA PROJ(&UPROJ.) DB(&UDB.)) MODE(FSCR)'
        O, 'PANEL(ORAMON)'
        P, 'CMD(%DELPHI)'
        C, 'CMD(%CASE PROJ(&UPROJ.) USER(&UID) PSW(&UPSW) DB(&UDB.)) MODE(FSCR)'
      ' ', ' '
        *, '?'
               )
)END
./ ADD NAME=ORAMON   0101-01016-01016-1416-00041-00041-00000-RACFPNL
)ATTR
% type(text)  intens(high)
+ type(text)  intens(low)
$ type(input) intens(non)
_ type(input) intens(high)
)BODY
%--- &ZSYSID ------------------- ORACLE MONITOR -------------------- &ZSYSID ---
%OPTION ===>_ZCMD
+
+
+
+
+
+
+
+
+              Enter the database to monitor:   _UDB +
+
+
+
+
+
+
+
+
+
+
+
+   Enter %END+ command to terminate.
)INIT
  .HELP = ORAHELP
  .CURSOR = UDB
)PROC
  VER(&UDB, PICT, AAAx)
  VPUT (UDB) PROFILE
  &ZCMD = TRUNC(&ZCMD, ' ')
  &ZSEL = TRANS(TRUNC(&ZCMD, '.')
      ' ', 'CMD(ORAMON &UDB)'
        *, '?'
               )
)END
./ ADD NAME=RACFP05  0101-01016-01016-1416-00027-00027-00000-RACFPNL
)ATTR
  ¢ TYPE(INPUT) INTENS(HIGH) PAD(NULLS)
)BODY
%-------------------------- RACF LISTING DEFAULTS  -----------------------------
%COMMAND ===>_ZCMD                                                             +
+
+LISTING DEFAULT OPTIONS
+-----------------------
+SYSOUT class    %===>_Z+             Default to Class V
+Local printer ID%===>_Z       +      Default to LOCAL
%                                                                              +
%
%   1 +ACCEPT      - Accept listing options
%   X +EXIT        - Terminate without generate listing
)INIT
  .HELP = RACFH051
  .ZVARS = '(LSTCLA LSTPID)'
  IF (&LSTCLA = '')       &LSTCLA = 'V'
  IF (&LSTPID = '')       &LSTPID = 'LOCAL'
  .CURSOR = ZCMD

)PROC
  &ZSEL = TRANS( TRUNC (&ZCMD,'.')
     1,'CMD(%TSOALST PRINTER(&LSTPID) OUTCLASS(&LSTCLA))'
     ' ',' '
     X,'EXIT')
)END
./ ADD NAME=SENDMSG  0101-01016-01016-1416-00038-00038-00000-RACFPNL
)ATTR
_ TYPE(INPUT) INTENS(LOW) JUST(LEFT)
)BODY
+----- &ZSYSID -----------%TSO MESSAGE SENDING FACILITY+-------- &ZSYSID ------
%                                                                              +
+   OPTION ====>_zcmd+
%
%
%      +ENTER%TSO+USERID   ===> _uid    +                                      +
%      +ENTER MESSAGE%(MAX 60 CHAR)
% ==> _msg                                                         +
%
%       1 +ACCEPT - Accept the input for the MESSAGE
%
%       X +EXIT - Terminate MESSAGE FACILITY
%
%
%
+  Enter%END+command to terminate MESSAGE FACILITY.
%
%
)INIT
  .HELP = OFFH0001
  &ZPRIM = YES        /* ALWAYS A PRIMARY OPTION MENU      */
)PROC
 ver(&uid,nonblank)
 ver(&msg,nonblank)
&ZQ = &Z
  IF (&zcmd ^= ' ')
    &ZQ = TRUNC(&zcmd,'.')
    IF (&ZQ = '   ')
      .MSG = ISRU000
  &ZSEL = TRANS( &ZQ
            1,'CMD(sendmsg &uid &msg)'
              ' ',' '
                X,'exit'
                *,'?' )
)END
./ ADD NAME=USRCNS   0101-01016-01016-1416-00048-00048-00000-RACFPNL
)ATTR
 _ TYPE(INPUT) INTENS(HIGH)  PADC(NULLS) JUST(LEFT) CAPS(ON)
 % TYPE(OUTPUT) INTENS(HIGH) PAD(' ')  JUST(ASIS) CAPS(ON)
 ? TYPE(OUTPUT) INTENS(HIGH) PAD('-')  JUST(ASIS) CAPS(ON)
 ¢ TYPE(OUTPUT) INTENS(HIGH) PAD(' ')  JUST(LEFT) CAPS(ON)
 + TYPE(OUTPUT) INTENS(LOW)  PAD(' ')  JUST(ASIS) CAPS(ON)
 \ TYPE(OUTPUT) INTENS(LOW)  PAD(' ')  JUST(LEFT) CAPS(ON)
 | TYPE(TEXT) INTENS(HIGH)   PAD(' ')  JUST(ASIS) CAPS(ON)
 ! TYPE(TEXT) INTENS(LOW)    PAD(' ')  JUST(ASIS) CAPS(ON)
)BODY
|------------------------- LIST MCS COMMAND OUTPUT -----------------------------
|C =>_ZCMD                                                           |S =>_A   |
|Mode:\MOD  |%SID     |
|Long C =>_OCMD
                                            !&ZTIME &ZDATE |&CER
|Display ===>  !Time Stamp _Z! System Name _Z! Job Name  _Z| Hold ===> _Z| (Y/N)
?ZX
)MODEL
&CVAR
)INIT
  .ZVARS = '(CDTS CDSN CDJN CDHD)'
  &ZX = '----'
  &OCMD = ' '
)REINIT
)PROC
  VER (&CDTS,LIST,Y,N)
  VER (&CDSN,LIST,Y,N)
  VER (&CDJN,LIST,Y,N)
  VER (&CDHD,LIST,Y,N)
  IF (&ZCMD = ' ')
    IF (&OCMD ^= ' ')
      &SCMD = &OCMD
      &ZCMD = &OCMD
  IF (&ZCMD = 'RETL')
    IF (&SCMD ^= ' ')
      &ZCMD = ' '
      &OCMD = &SCMD
      REFRESH (OCMD ZCMD)
      &ZEDSMSG = 'O.K.'
      &ZEDLMSG = 'Enter / Modify retrieved command'
      .MSG = ISRZ000
    ELSE
      &ZCMD = ' '
      REFRESH ZCMD
      &ZEDSMSG = 'Nothing Saved'
      &ZEDLMSG = 'Nothing to retrieve - Long commands have not been used'
      .MSG = ISRZ001
)END
./ ADD NAME=USRTERM  0101-01016-01016-1416-00077-00077-00000-RACFPNL
)ATTR
% type(text)  intens(high)
+ type(text)  intens(low)
$ type(input) intens(non)
_ type(input) intens(high)
)BODY
%-------- &zsysid ---------------+Administrator%---------------- &zsysid -------
%OPTION ===>_ZCMD                                           +USERID   -%&ZUSER
+                                                           +TIME     - &ZTIME
+    USERID  :_UID    +                                     +TERMINAL - &ZTERM
+    NAME    :_USNAME              +  %Name & Surname (20 Characters) +
+    PASSWORD:$UPSW    +
+    TERMINAL:_TERM    + %Terminal ID of the user (REQUIRED)+
+
+------- ENTER USERID AND PASSWORD ---------------------------------------------
%
%   1 +ADD      - Add a new RACF user
%   2 +CHANGE   - Change a users password / name
%   3 +DELETE   - Delete a user
%   4 +LISTS    - List a user
%   5 +LIST ALL - List all users
%
%   C +JOB OUTPUT    - Check job output
%   X +EXIT          - Terminate ISPF using log and list defaults
%
+   Enter %END+ command to terminate.
)INIT
  .HELP = RACFH00     /* CHANGE THIS TO REFLECT HELP SCREEN*/
  &ZPRIM = YES        /* ALWAYS A PRIMARY OPTION MENU      */
  &ZHTOP = RACFH00    /* TUTORIAL TABLE OF CONTENTS        */
  &ZHINDEX = RACFH00  /* TUTORIAL INDEX - 1ST PAGE         */
  &ZSCLMPRJ = &Z      /* TUTORIAL INDEX - 1ST PAGE     @L1A*/
  &ZPFCTL = OFF
  VPUT (ZHTOP,ZHINDEX,ZSCLMPRJ,ZPFCTL) PROFILE
  &UID = ' '
  &UPSW = ' '
  &USNAME = ' '
  .CURSOR = ZCMD
)PROC
  VER(&ZCMD, NONBLANK)
  VER(&TERM, NONBLANK)
&ZQ = &Z
  IF (&ZCMD ^= ' ')
    &ZQ = TRUNC(&ZCMD,'.')
    IF (&ZQ = ' ')
      .MSG = ISRU000
  &OPTN = TRUNC(&ZCMD,'.')
  IF (&OPTN = 1,2,3,4)
     VER(&UID, NONBLANK)
     VER(&UID, LEN, '=', 7)
     VER(&TERM, LEN, '=', 8)
     &NR = TRUNC(&UID,3)
     VER(&NR, PICT, AAA)
  IF (&OPTN = 1)
     VER(&USNAME,NONBLANK, MSG = ICHM413A)
     VER(&USNAME,LEN, '<=', 21)
     VER(&UPSW, NONBLANK)
     &QNAME = TRUNC(&USNAME,1) /* get first char        @ZA85750*/
     IF (&QNAME = '''')        /* is first char a quote @ZA85750*/
       .MSG = ICHM413A         /* issue message         @ZA85750*/
       .CURSOR = USNAME        /* point to name         @ZA85750*/
  &VALCHK = ICHC41             /* check for end quote   @ZA91987*/
  IF (&OPTN = 2)
     VER(&UPSW, NONBLANK)
  VPUT (UID NAME UPSW) PROFILE
  &ZSEL = TRANS( &ZQ
   1,'CMD(%TSOUADD &UID:&USNAME:&UPSW)'
   2,'CMD(%COMRES &UID:&USNAME:&UPSW:&TERM)'
   3,'CMD(%TSOUDEL &UID)'
   4,'CMD(%TSOULST &UID)'
   5,'PANEL(RACFP05)'
   C,'PGM(isfisp) parm(H)'
   ' ',' '
   X,'exit'
   *,'?' )
  &ZTRAIL = .TRAIL
)END
