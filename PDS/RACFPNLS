./ ADD NAME=ISPPFT03 0101-01016-01016-1416-00102-00102-00000-RACFPNL
%--------------  SPECIFY DISPOSITION OF LOG AND LIST DATA SETS  ----------------
%COMMAND ===>_ZCMD                                                             +
+
+LOG OPTIONS FOR THIS SESSION           LIST OPTIONS FOR THIS SESSION
+----------------------------           -----------------------------
+Process option  %===>_ZLOGFDDV+        Process option  %===>_ZLSTFDDV+
+SYSOUT class    %===>_ZLOGCLA        + SYSOUT class    %===>_ZLSTCLA        +
+Local printer ID%===>_ZLOGPID +        Local printer ID%===>_ZLSTPID +
+
+VALID PROCESS OPTIONS:
%    PD+- Print data set and delete
%    D +- Delete data set without printing
%    K +- Keep data set (allocate same data set in next session)
%    KN+- Keep data set and allocate new data set in next session
+
+  Press%ENTER+key to complete ISPF termination.
+  Enter%END+command to return to the primary option menu.
+
+JOB STATEMENT INFORMATION:  (Required for system printer)
%  ===>_ZLLGJOB1
%  ===>_ZLLGJOB2
%  ===>_ZLLGJOB3
%  ===>_ZLLGJOB4
)INIT
  .HELP = ISP90110
  If (&ZLOGFDSP = J,L)                  /* Initialize log process option     */
    &ZLOGFDDV = PD
  Else
    If (&ZLOGFDSP = R)
      &ZLOGFDDV = KN
    Else
      If (&ZLOGFDSP = D,K)
        &ZLOGFDDV = &ZLOGFDSP
      Else
  If (&ZLSTFDSP = J,L)                  /* Initialize list process option    */
    &ZLSTFDDV = PD
  Else
    If (&ZLSTFDSP = R)
      &ZLSTFDDV = KN
    Else
      If (&ZLSTFDSP = D,K)
        &ZLSTFDDV = &ZLSTFDSP
      Else
  If (&ZLOGFDDV ^= ' ')
    If (&ZLSTFDDV = ' ')
      .CURSOR = ZLSTFDDV
    Else
      .CURSOR = ZLOGFDDV
  Else
    .CURSOR = ZLOGFDDV
)REINIT
  If (.CURSOR = ' ')
    .CURSOR = ZLOGFDDV
    If (&ZLOGFDDV ^= ' ')
      If (&ZLSTFDDV = ' ')
        .CURSOR = ZLSTFDDV
      Else
    Else
  Else
)PROC
 If (.RESP = ENTER)
   If (&ZVERB ^= ' ' )
     .MSG = ISPZ002
   Else
     If (&ZCMD ^= ' ' )
       .MSG = ISPZ001
     Else
 Else
 Ver (&ZLOGFDDV NB LIST PD,D,K,KN)      /* Validate log options              */
 If (&ZLOGFDDV = 'PD')                  /* If print specified and            */
   If (&ZLOGPID ^= ' ' )                /*   both local printer and          */
     If (&ZLOGCLA ^= ' ')               /*   sysout class specified then     */
       .MSG = ISPP032                   /*  error - duplicate specification  */
     Else
   Else                                 /* If print and neither local printer*/
     If (&ZLOGCLA = ' ')                /*   or sysout class specified then  */
       .MSG = ISPP031                   /*  error - must specify one parm    */
     Else
   &ZLOGFDDV = J
 Else
   If (&ZLOGFDDV = 'KN')
     &ZLOGFDDV = R
   Else
 &ZLOGFDSP = &ZLOGFDDV
 Ver (&ZLSTFDDV NB LIST PD,D,K,KN)      /* Validate list process options     */
 If (&ZLSTFDDV = 'PD')                  /* If print specified and            */
   If (&ZLSTPID ^= ' ' )                /*   both local printer and          */
     If (&ZLSTCLA ^= ' ')               /*   sysout class specified then     */
       .MSG = ISPP032                   /*  error - duplicate specification  */
     Else
   Else                                 /* If print and neither local printer*/
     If (&ZLSTCLA = ' ')                /*   or sysout class specified then  */
       .MSG = ISPP031                   /*  error - must specify one parm    */
     Else
   &ZLSTFDDV = J
 Else
   If (&ZLSTFDDV = 'KN')
     &ZLSTFDDV = R
   Else
 &ZLSTFDSP = &ZLSTFDDV
)END
