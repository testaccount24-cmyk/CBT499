)INIT
  &USERID = ' '
  .HELP = RACFH07
  .ZVARS = '(req reas ct1 ct2 ct3 ct4 ct5 ct6 ct7 ct8 ct9 ct10 +
  ct11 ct12 ct13 ct14 ct15 ct16 ct17 ct18 ct19 ct20)'
  IF (&ct1 = '')       &ct1 = ' '
  IF (&ct2 = '')       &ct2 = ' '
  IF (&ct3 = '')       &ct3 = ' '
  IF (&ct4 = '')       &ct4 = ' '
  IF (&ct5 = '')       &ct5 = ' '
  IF (&ct6 = '')       &ct6 = ' '
  IF (&ct7 = '')       &ct7 = ' '
  IF (&ct8 = '')       &ct8 = ' '
  IF (&ct9 = '')       &ct9 = ' '
  IF (&ct10 = '')      &ct10 = ' '
  IF (&ct11 = '')      &ct11 = ' '
  IF (&ct12 = '')      &ct12 = ' '
  IF (&ct13 = '')      &ct13 = ' '
  IF (&ct14 = '')      &ct14 = ' '
  IF (&ct15 = '')      &ct15 = ' '
  IF (&ct16 = '')      &ct16 = ' '
  IF (&ct17 = '')      &ct17 = ' '
  IF (&ct18 = '')      &ct18 = ' '
  IF (&ct19 = '')      &ct19 = ' '
  IF (&ct20 = '')      &ct20 = ' '
)REINIT
)PROC
  VER(&ZCMD, NONBLANK)
  IF (&ZQ = ' ')
     .MSG = ISRU000
  &OPTN = TRUNC(&ZCMD,'.')
  IF (&OPTN = 1)
     VER(&req, NONBLANK)
     VER(&reas, NONBLANK)
     VER(&ct1, NONBLANK)
     IF (&CT1 ^= '')
      VER(&ct1, LEN, '>', 3)
     IF (&ct2 ^= '')
      VER(&ct2, LEN, '>', 3)
     IF (&CT3 ^= '')
      VER(&ct3, LEN, '>', 3)
     IF (&CT4 ^= '')
      VER(&ct4, LEN, '>', 3)
     IF (&CT5 ^= '')
      VER(&ct5, LEN, '>', 3)
     IF (&CT6 ^= '')
      VER(&ct6, LEN, '>', 3)
     IF (&CT7 ^= '')
      VER(&ct7, LEN, '>', 3)
     IF (&CT8 ^= '')
      VER(&ct8, LEN, '>', 3)
     IF (&CT9 ^= '')
      VER(&ct9, LEN, '>', 3)
     IF (&CT10 ^= '')
      VER(&ct10, LEN, '>', 3)
     IF (&CT11 ^= '')
      VER(&ct11, LEN, '>', 3)
     IF (&CT12 ^= '')
      VER(&ct12, LEN, '>', 3)
     IF (&CT13 ^= '')
      VER(&ct13, LEN, '>', 3)
     IF (&CT14 ^= '')
      VER(&ct14, LEN, '>', 3)
     IF (&CT15 ^= '')
      VER(&ct15, LEN, '>', 3)
     IF (&CT16 ^= '')
      VER(&ct16, LEN, '>', 3)
     IF (&CT17 ^= '')
      VER(&ct17, LEN, '>', 3)
     IF (&CT18 ^= '')
      VER(&ct18, LEN, '>', 3)
     IF (&CT19 ^= '')
