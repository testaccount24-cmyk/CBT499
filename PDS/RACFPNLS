  IF (&OPTN = 5,6)
     VER(&prn, NONBLANK)
  &ZSEL = TRANS( &ZQ
   1,'CMD(%kriutil create)'
   2,'CMD(%kriutil list)'
   3,'CMD(%kriutil summary)'
   4,'panel(isp@kric)'
   5,'panel(isp@krid)'
   6,'CMD(%kriutil print1:&prn)'
