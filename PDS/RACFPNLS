)ATTR
% type(text)  intens(high)
+ type(text)  intens(low)
$ type(input) intens(non)
_ type(input) intens(high)
)BODY
%-------- &ZSYSID -------+Complete - Racf Administrator%--------- &ZSYSID ------
%OPTION ===>_ZCMD                                           +USERID   -%&ZUSER
+                                                           +TIME     - &ZTIME
+    USERID  :_UID   +                                      +TERMINAL - &ZTERM
+    FORCENR :_NUM    +        %Forcenumber
+    SURNAME :_USNAME      +   %Surname
+    PASSWORD:$UPSW    +
+
+------- ENTER USERID AND PASSWORD ---------------------------------------------
%
%   1 +ADD      - Add a new RACF user
%   2 +CHANGE   - Change a users password / name
%   3 +DELETE   - Delete a user
%   4 +LISTS    - List a user
%   5 +LIST ALL - List all users
%   6 +FIND     - Find spesific user by giving forcenumber
%   7 %LAST LOG - List users with last logon before specified date
%
%   C +JOB OUTPUT    - Check job output
%   X +EXIT          - Terminate ISPF using log and list defaults
%
+   Enter %END+ command to terminate.
)INIT
  .HELP = comH00     /* CHANGE THIS TO REFLECT HELP SCREEN*/
  &ZPRIM = YES        /* ALWAYS A PRIMARY OPTION MENU      */
  &ZHTOP = comH00    /* TUTORIAL TABLE OF CONTENTS        */
  &ZHINDEX = comH00  /* TUTORIAL INDEX - 1ST PAGE         */
  &ZSCLMPRJ = &Z      /* TUTORIAL INDEX - 1ST PAGE     @L1A*/
  &ZPFCTL = OFF
  VPUT (ZHTOP,ZHINDEX,ZSCLMPRJ,ZPFCTL) PROFILE
  &upsw = ' '
  .CURSOR = ZCMD
)PROC
  VER(&ZCMD, NONBLANK)
&ZQ = &Z
  IF (&ZCMD ^= ' ')
    &ZQ = TRUNC(&ZCMD,'.')
    IF (&ZQ = ' ')
      .MSG = ISRU000
  &OPTN = TRUNC(&ZCMD,'.')
  IF (&OPTN = 6)
     VER(&NUM, NONBLANK)
  IF (&OPTN = 1,2,3,4)
     VER(&UID, NONBLANK)
     VER(&UID, LEN, '=', 6)
     &NR = TRUNC(&UID,2)
     VER(&NR, PICT, AA)
  IF (&OPTN = 1)
     VER(&NUM,NONBLANK, MSG = ICHM413A)
     VER(&USNAME,NONBLANK, MSG = ICHM413A)
     VER(&USNAME,LEN, '<=', 21)
     VER(&UPSW, NONBLANK)
     &QNAME = TRUNC(&USNAME,1) /* get first char        @ZA85750*/
     IF (&QNAME = '''')        /* is first char a quote @ZA85750*/
       .MSG = ICHM413A         /* issue message         @ZA85750*/
       .CURSOR = USNAME        /* point to name         @ZA85750*/
  &VALCHK = ICHC41             /* check for end quote   @ZA91987*/
  IF (&OPTN = 2)
     VER(&UPSW, NONBLANK)
  VPUT (UID NUM USNAME UPSW) PROFILE
  &ZSEL = TRANS( &ZQ
   1,'CMD(%comUADD &UID:&NUM &USNAME:&UPSW)'
   2,'CMD(%comURES &UID:&NUM &USNAME:&UPSW)'
   3,'CMD(%comUDEL &UID)'
   4,'CMD(%comULST &UID)'
   5,'PANEL(comP05)'
   6,'cmd(%lstforce &NUM)'
   7,'PANEL(comP06)'
   C,'PGM(isfisp) parm(H)'
   ' ',' '
   X,'EXIT'
   *,'?' )
  &ZTRAIL = .TRAIL
)END
