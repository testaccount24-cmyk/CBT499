                T,'PANEL(ISP@TSP)'
              ' ',' '
                X,'EXIT'
                *,'?' )
  &ZTRAIL = .TRAIL
)END
./ ADD NAME=ISP@THEA 0101-01016-01016-1416-00081-00081-00000-RACFPNL
)ATTR
% type(text)  intens(high)
+ type(text)  intens(low)
$ type(input) intens(non)
_ type(input) intens(high)
)BODY
%--------&ZSYSID-----------+THELIVE Racf Administrator%---------&ZSYSID---------
%OPTION ===>_ZCMD+                                           USERID   -%&ZUSER
+                                                           +TIME     - &ZTIME
+    USERID  :_UID     +                                    +TERMINAL - &ZTERM
+    NAME    :_USNAME              +  %(Name and Surname) +
+    GROUP   :_GRPNME +               %(THELIVE,MID or MOD)+
+    PASSWORD:$UPSW    +
+    OPIDENT :_RG +                   %(C07,F08,ETC....)+
+
%    ----------------------------------------------------------------
%   |%OPIDENT+field is the cics OPID created before hand in the cics%ø
%    ----------------------------------------------------------------
+
+------- ENTER USERID AND PASSWORD ---------------------------------------------
%
%   1 +ADD     %-+Add a new RACF user -
%   2 +CHANGE  %-+Change a users password / name
%   3 +DELETE  %-+Delete a user
%   4 +LISTS   %-+List a user
%   5 +CHANGE  %-+Change a users OPIDENT
%
%   X +EXIT          - Return to previous panel
%
+   Enter %END+ command to terminate.
)INIT
  .HELP = RACFH00     /* CHANGE THIS TO REFLECT HELP SCREEN*/
  &ZHTOP = RACFH00    /* TUTORIAL TABLE OF CONTENTS        */
  &ZHINDEX = RACFH00  /* TUTORIAL INDEX - 1ST PAGE         */
  &ZSCLMPRJ = &Z      /* TUTORIAL INDEX - 1ST PAGE     +L1A*/
  &ZPFCTL = OFF
  VPUT (ZHTOP,ZHINDEX,ZSCLMPRJ,ZPFCTL) PROFILE
  &UPSW = ' '
  &RG = ' '
  .CURSOR = ZCMD
)PROC
  VER(&ZCMD, NONBLANK)
&ZQ = &Z
  IF (&ZCMD ^= ' ')
    &ZQ = TRUNC(&ZCMD,'.')
    IF (&ZQ = ' ')
        .MSG = ISRU000
  &OPTN = TRUNC(&ZCMD,'.')
  IF (&OPTN = 1,2,3,4)
     VER(&UID, NONBLANK)
     &jst = TRUNC(&UID,3)     /* get third char for uid */
     &just = TRUNC(&UID,4)    /* get fourth char for uid */
  IF (&OPTN = 1)
     VER(&USNAME,NONBLANK, MSG = ICHM413A)
     VER(&USNAME,LEN, '<=', 21)
     VER(&GRPNME, NONBLANK)
     VER(&UPSW, NONBLANK)
     if (&JST = JST)
       VER(&RG, NONBLANK)
     else
     if (&JUST = JUST)
        VER(&RG, NONBLANK)
     &QNAME = TRUNC(&USNAME,1) /* get first char        +ZA85750*/
     IF (&QNAME = '''')        /* is first char a quote +ZA85750*/
       .MSG = ICHM413A         /* issue message         +ZA85750*/
       .CURSOR = USNAME        /* point to name         +ZA85750*/
  &VALCHK = ICHC41             /* check for end quote   +ZA91987*/
  IF (&OPTN = 2)
     VER(&UPSW, NONBLANK)
  VPUT (UID NAME UPSW) PROFILE
  &ZSEL = TRANS( &ZQ
   1,'CMD(%THEADD &UID:&USNAME:&GRPNME:&UPSW:&RG)'
   2,'CMD(%THERES &UID:&USNAME:&UPSW)'
   3,'CMD(%THEDEL &UID)'
   4,'CMD(%THELST &UID)'
   5,'CMD(%THEOPT &UID:&RG)'
   ' ',' '
   X,'EXIT'
   *,'?' )
  &ZTRAIL = .TRAIL
)END
