./ ADD NAME=ISRUDA2  0101-01016-01016-1416-00055-00055-00000-RACFPNL
)ATTR
  ^ TYPE(INPUT) INTENS(NON)
)BODY
%----------------------------  DATA SET UTILITY  -------------------------------
%OPTION  ===>_ZCMD                                                             +
%
%   C+- Catalog data set
%   R+- Rename entire data set               %U+- Uncatalog data set
%   D+- Delete entire data set               %S+- Data set information (short)
%   blank+- Data set information
+
+ISPF LIBRARY:
+   PROJECT%===>_PRJ0    +
+   GROUP  %===>_LIB0    +
+   TYPE   %===>_TYP0    +
+
+OTHER PARTITIONED OR SEQUENTIAL DATA SET:
+   DATA SET NAME %===>_DSN                                                    +
+   VOLUME SERIAL %===>_VOL   +  (If not cataloged, required for option "C")
+
+DATA SET PASSWORD%===>^PSWD    +(If password protected)
)INIT
  .HELP = ISR32000
  &ZCMD = TRANS(&OPT N,R E,D I,' ' V,U *,*)
  IF ( &DSN = ' ' )
    &VOL = &Z
  IF ( &DSN ^= ' ' )
    .CURSOR = ZCMD
  IF ( .CURSOR = ' ' )
    .CURSOR = ZCMD
)PROC
  &OPT = TRANS(&ZCMD N,# E,# I,# V,# R,N D,E ' ',I U,V *,*)
  VER(&OPT,LIST,N,E,I,C,V,S)
  IF ( &OPT = C )
    VER(&VOL,NB)
  IF ( &OPT = V )
    IF (&VOL ^= ' ' )
      .MSG = ISRU230
  IF ( &DSN = ' ' )
    VER(&PRJ0,NB)
    VER(&LIB0,NB)
    VER(&TYP0,NB)
    IF ( &OPT ^= C )
      IF (&VOL ^= ' ' )
        .MSG = ISRU232
  IF ( &DSN ^= ' ' )                    /* DSN SPECIFIED ??       @M1A*/
    &ZFC = TRUNC(&DSN,1)                /* IF FIRST CHARACTER     @M1A*/
    IF (&ZFC = '''')                    /*  OF DSN IS "'" CHECK   @M1A*/
      &ZREM = .TRAIL                    /*  TO SEE IF LAST "'"    @M1A*/
      &ZREM1 = TRUNC(&ZREM,'''')        /*  IS MISSING.           @M1A*/
      IF (&ZREM1 = &ZREM)               /*  IF LAST "'" MISSING   @M1A*/
        &DSN = '&DSN&ZFC'               /*    ADD IT TO THE END   @M1A*/
    VER (&DSN,DSNAME)
  VPUT ( PRJ0 LIB0 TYP0 ) PROFILE
)END
./ ADD NAME=ISRUD2   0101-01016-01016-1416-00061-00061-00000-RACFPNL
)ATTR
  ^ TYPE(INPUT) INTENS(NON)
)BODY
%----------------------------  DATA SET UTILITY  -------------------------------
%OPTION  ===>_ZCMD                                                             +
%
%   C+- Catalog data set                     %U+- Uncatalog data set
%   R+- Rename entire data set               %S+- Data set information (short)
%   D+- Delete entire data set               %blank+- Data set information
%
+
+ISPF LIBRARY:
+   PROJECT%===>_PRJ0    +
+   GROUP  %===>_LIB0    +
+   TYPE   %===>_TYP0    +
+
+OTHER PARTITIONED OR SEQUENTIAL DATA SET:
+   DATA SET NAME %===>_DSN                                                    +
+   VOLUME SERIAL %===>_VOL   +  (If not cataloged, required for option "C")
+
+DATA SET PASSWORD%===>^PSWD    +(If password protected)
)INIT
  .HELP = ISR32000
  &ZCMD = TRANS(&OPT N,R E,D I,' ' V,U *,*)
  IF ( &DSN = ' ' )
    &VOL = &Z
  IF ( &DSN ^= ' ' )
    .CURSOR = ZCMD
  IF ( .CURSOR = ' ' )
    .CURSOR = ZCMD
)PROC
  &OPT = TRANS(&ZCMD N,# E,# I,# V,# R,N D,E ' ',I U,V *,*)
  VER(&OPT,LIST,N,E,I,C,V,S)
  IF ( &OPT = C )
    VER(&VOL,NB)
  IF ( &OPT = V )
    IF (&VOL ^= ' ' )
      .MSG = ISRU230
  IF ( &DSN = ' ' )
    VER(&PRJ0,NB)
    VER(&LIB0,NB)
    VER(&TYP0,NB)
    IF ( &OPT ^= C )
      IF (&VOL ^= ' ' )
        .MSG = ISRU232
  IF ( &DSN ^= ' ' )                    /* DSN SPECIFIED ??       @M1A*/
    &ZFC = TRUNC(&DSN,1)                /* IF FIRST CHARACTER     @M1A*/
    IF (&ZFC = '''')                    /*  OF DSN IS "'" CHECK   @M1A*/
      &ZREM = .TRAIL                    /*  TO SEE IF LAST "'"    @M1A*/
      &ZREM1 = TRUNC(&ZREM,'''')        /*  IS MISSING.           @M1A*/
      IF (&ZREM1 = &ZREM)               /*  IF LAST "'" MISSING   @M1A*/
        &DSN = '&DSN&ZFC'               /*    ADD IT TO THE END   @M1A*/
    VER (&DSN,DSNAME)
  VPUT ( PRJ0 LIB0 TYP0 ) PROFILE
)END
