./ ADD NAME=ISP@KRIB 0101-01016-01016-1416-00050-00050-00000-RACFPNL
%------- &ZSYSID ------------  Printer Information ------------ &ZSYSID --------
%OPTION  ===>_ZCMD+ (Option = ADD/DEL/LST,PRT)                                 +
%                                                           +USERID   -%&ZUSER
%
%   Printer id ==>_pid     + (Required field for%Add & Del)
%   Webnr      ==>_web     + (Required field for%Add)
%   Location   ==>_loc                                                         +
%
%  +ADD %- + To add or modify exiting printer information
%  +DEL %- + To delete exiting printer information
%  +LST %- + To list exiting printer information on screen
%  +PRT %- + To print exiting printer information to your printer
%
%
%   Your Printerid+===>_PRN     +
%
%
+Enter%END+or%X +to exit.
)INIT
  .CURSOR = ZCMD
)PROC
  VER(&ZCMD, NONBLANK)
&ZQ = &Z
  IF (&ZCMD ^= ' ')
    &ZQ = TRUNC(&ZCMD,'.')
    IF (&ZQ = ' ')
        .MSG = ISRU000
  &OPTN = TRUNC(&ZCMD,'.')
  IF (&OPTN = ADD)
     VER(&pid, NONBLANK)
     VER(&web, NONBLANK)
     VER(&pid, len, '=', 8)
     VER(&web, len, '=', 8)
     VER(&loc, NONBLANK)
  IF (&OPTN = DEL)
     VER(&pid, NONBLANK)
     VER(&pid, len, '=', 8)
  IF (&OPTN = PRT)
     VER(&prn, NONBLANK)
     VER(&prn, len, '=', 8)
  &ZSEL = TRANS( &ZQ
  ADD,'CMD(%kripid add:&pid:&web:&loc)'
  DEL,'CMD(%kripid del:&pid)'
  LST,'CMD(%kripid list)'
  PRT,'CMD(%kripid prt:&prn)'
   ' ',' '
   X,'EXIT'
   *,'?' )
  &ZTRAIL = .TRAIL
)END
