    &ZQ = TRUNC(&ZCMD,'.')
    IF (&ZQ = ' ')
        .MSG = ISRU000
  &OPTN = TRUNC(&ZCMD,'.')
  IF (&OPTN = S)
      VER (&LUNAME, NONBLANK)
     .CURSOR = LUNAME
  &VALCHK = ICHC41
  vput ( luname ) profile
  &ZSEL = TRANS( &ZQ
   T,'CMD(TSP)'
   T2,'CMD(TSP TSPRINT2)'
   T3,'CMD(TSP TSPRINT3)'
   T4,'CMD(TSP TSPRINT4)'
   T5,'CMD(TSP TSPRINT5)'
   T6,'CMD(TSP TSPRINT6)'
   T7,'CMD(TSP TSPRINT7)'
   T8,'CMD(TSP TSPRINT8)'
   T9,'CMD(TSP TSPRINT9)'
   T0,'CMD(TSP TSPOFS)'
    S,'CMD(TSPFIND &LUNAME)'
    ' ',' '
    X,'exit'
    *,'?' )
  &ZTRAIL = .TRAIL
)END
