./ ADD NAME=ISPPFT02 0101-01016-01016-1416-00066-00066-00000-RACFPNL
%-------------------  SPECIFY DISPOSITION OF LIST DATA SET ---------------------
%COMMAND ===>_ZCMD                                                             +
+
+LOG DATA SET OPTIONS NOT AVAILABLE     LIST DATA SET DISPOSITION
+----------------------------------     -------------------------
+                                       Process option  %===>_ZLSTFDDV +
+                                       SYSOUT class    %===>_ZLSTCLA        +
+                                       Local printer ID%===>_ZLSTPID +
+
+VALID PROCESS OPTIONS:
%    PD+- Print data set and delete
%    D +- Delete data set without printing
%    K +- Keep data set (allocate same data set in next session)
%    KN+- Keep data set and allocate new data set in next session
+
+  Press%ENTER+key to complete ISPF termination.
+  Enter%END+command to return to the primary option menu.
+
+JOB STATEMENT INFORMATION:  (Required for system printer)
%  ===>_ZLLGJOB1
%  ===>_ZLLGJOB2
%  ===>_ZLLGJOB3
%  ===>_ZLLGJOB4
)INIT
  .HELP = ISP90110
  .CURSOR = ZLSTFDDV
  If (&ZLSTFDSP = J,L)                  /* Initialize DISPLAY process option */
    &ZLSTFDDV = PD
  Else
    If (&ZLSTFDSP = R)
      &ZLSTFDDV = KN
    Else
      If (&ZLSTFDSP = D,K)
        &ZLSTFDDV = &ZLSTFDSP
      Else
)REINIT
  If (.CURSOR = ' ')
    .CURSOR = ZLSTFDDV
  Else
)PROC
 If (.RESP = ENTER)
   If (&ZVERB ^= ' ' )
     .MSG = ISPZ002
   Else
     If (&ZCMD ^= ' ' )
       .MSG = ISPZ001
     Else
 Else
 Ver (&ZLSTFDDV NB LIST PD,D,K,KN)
 If (&ZLSTFDDV = 'PD')                  /* If print specified and            */
   If (&ZLSTPID ^= ' ' )                /*   both local printer and          */
     If (&ZLSTCLA ^= ' ')               /*   sysout class specified then     */
       .MSG = ISPP032                   /*  error - duplicate specification  */
     Else
   Else                                 /* If print and neither local printer*/
     If (&ZLSTCLA = ' ')                /*   or sysout class specified then  */
       .MSG = ISPP031                   /*  error - must specify one parm    */
     Else
   &ZLSTFDDV = J
 Else
   If (&ZLSTFDDV = 'KN')
     &ZLSTFDDV = R
   Else
 &ZLSTFDSP = &ZLSTFDDV
)END
