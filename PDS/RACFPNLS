./ ADD NAME=COMP05   0101-01016-01016-1416-00027-00027-00000-RACFPNL
)ATTR
  ¢ TYPE(INPUT) INTENS(HIGH) PAD(NULLS)
)BODY
%-------------------------- RACF LISTING DEFAULTS  -----------------------------
%COMMAND ===>_ZCMD                                                             +
+
+LISTING DEFAULT OPTIONS
+-----------------------
+SYSOUT class    %===>_Z+             Default to Class V
+Local printer ID%===>_Z       +      Default to LOCAL
%                                                                              +
%
%   1 +ACCEPT      - Accept listing options
%   X +EXIT        - Terminate without generate listing
)INIT
  .HELP = RACFH051
  .ZVARS = '(LSTCLA LSTPID)'
  IF (&LSTCLA = '')       &LSTCLA = 'V'
  IF (&LSTPID = '')       &LSTPID = 'LOCAL'
  .CURSOR = ZCMD

)PROC
  &ZSEL = TRANS( TRUNC (&ZCMD,'.')
     1,'CMD(%COMALST PRINTER(&LSTPID) OUTCLASS(&LSTCLA))'
     ' ',' '
     X,'EXIT')
)END
./ ADD NAME=COMP06   0101-01016-01016-1416-00033-00033-00000-RACFPNL
)ATTR
  ¢ TYPE(INPUT) INTENS(HIGH) PAD(NULLS)
)BODY
%-------------------------- RACF LISTING DEFAULTS  -----------------------------
%COMMAND ===>_ZCMD+                                                            +
+
+
+
+LAST ACCESS DATE   %===>_Z         +   Date in the form YYYY-MM-DD
+GROUP to be listed %===>_Z       +     NO DEFAULT
%                                                                              +
% For a list of valid groups                                                   +
%                                                                              +
% Phone Supt E Ross      (012) 421-0654/5                                      +
%       Supt H Potgieter (012) 421-0767                                        +
%                                                                              +
% OR FAX (012) 421-0606                                                        +
%
%   1 +ACCEPT      - Accept listing options
%   X +EXIT        - Terminate without generate listing
)INIT
  .HELP = RACFH051
  .ZVARS = '(LSTCLA LSTPID)'
  IF (&LSTCLA = '')       &LSTCLA = '1990-01-01'
  IF (&LSTPID = '')       &LSTPID = 'UNKNOWN'
  .CURSOR = ZCMD

)PROC
  &ZSEL = TRANS( TRUNC (&ZCMD,'.')
     1,'CMD(%RACFEXEC &LSTCLA &LSTPID)'
     ' ',' '
     X,'EXIT')
)END
./ ADD NAME=HSMUTIL  0101-01016-01016-1416-00058-00058-00000-RACFPNL
)BODY
%-----&ZSYSID--------------  HSM DATASET UTILITY  -------------------&ZSYSID----
%OPTION  ===>_ZCMD                                                             +
%
%   L / Blank+- List HSM dataset          %R+- Recover HSM data set
%
%
%
+   DATA SET NAME %===>_DSN                                                    +
%
%
%
%   Required if recovering a dataset:
+   ---------------------------------
+   GENERATION    %===>_GEN+
+   VOLUME SERIAL %===>_VOL   +%(Optional)
+   UNIT NAME     %===>_UNT +  %(Optional)
+
+   REPLACE       %===>_A+
+
+   NEW DATA SET NAME %===>_DSN2                                               +
+
)INIT
  .HELP = ISR32000
  IF ( &DSN = ' ' )
    &VOL = &Z
  IF ( &DSN ^= ' ' )
    .CURSOR = ZCMD
  IF ( .CURSOR = ' ' )
    .CURSOR = ZCMD
  &a = 'N'
  &gen = 000
)PROC
&ZQ = TRUNC(&ZCMD,'.')
IF (&ZQ = ' ')
  &ZQ = 'L'
    ver (&dsn,nonblank)
    ver (&dsn,dsname)
    ver (&a,nonblank)
if (&zq = 'R')
 if (&a = 'N')
   ver (&gen,range,000,009)
   ver (&gen,nonblank)
   ver (&dsn2,nonblank)
   ver (&dsn2,dsname)
 if (&zq = 'R')
  if (&a = 'Y')
   ver (&gen,range,000,009)
   ver (&gen,nonblank)
   ver (&a,nonblank)
VPUT ( DSN ) PROFILE
  &ZSEL = TRANS( &ZQ
                L,'cmd(hsmcmd LIST:&dsn)'
                R,'cmd(hsmcmd RECOVER:&dsn:&gen:&vol:&unt:&a:&dsn2)'
              ' ',' '
                X,'exit'
                *,'?' )
)END
./ ADD NAME=ISP@ADMN 0101-01016-01016-1416-00074-00074-00000-RACFPNL
)ATTR
% type(text)  intens(high)
+ type(text)  intens(low)
$ type(input) intens(non)
_ type(input) intens(high)
)BODY
%-------- &zsysid ---------------+Administrator%---------------- &zsysid -------
%OPTION ===>_ZCMD                                           +USERID   -%&ZUSER
+                                                           +TIME     - &ZTIME
+    USERID  :_UID    +                                     +TERMINAL - &ZTERM
+    NAME    :_USNAME              +  %Name & Surname (20 Characters) +
+    PASSWORD:$UPSW    +
+
+------- ENTER USERID AND PASSWORD ---------------------------------------------
%
%   1 +ADD      - Add a new RACF user
%   2 +CHANGE   - Change a users password / name
%   3 +DELETE   - Delete a user
%   4 +LISTS    - List a user
%   5 +LIST ALL - List all users
%
%   C +JOB OUTPUT    - Check job output
%   X +EXIT          - Terminate ISPF using log and list defaults
%
+   Enter %END+ command to terminate.
)INIT
  .HELP = RACFH00     /* CHANGE THIS TO REFLECT HELP SCREEN*/
  &ZPRIM = YES        /* ALWAYS A PRIMARY OPTION MENU      */
  &ZHTOP = RACFH00    /* TUTORIAL TABLE OF CONTENTS        */
  &ZHINDEX = RACFH00  /* TUTORIAL INDEX - 1ST PAGE         */
  &ZSCLMPRJ = &Z      /* TUTORIAL INDEX - 1ST PAGE     @L1A*/
  &ZPFCTL = OFF
  VPUT (ZHTOP,ZHINDEX,ZSCLMPRJ,ZPFCTL) PROFILE
  &UID = ' '
  &UPSW = ' '
  &USNAME = ' '
  .CURSOR = ZCMD
)PROC
  VER(&ZCMD, NONBLANK)
&ZQ = &Z
  IF (&ZCMD ^= ' ')
    &ZQ = TRUNC(&ZCMD,'.')
    IF (&ZQ = ' ')
      .MSG = ISRU000
  &OPTN = TRUNC(&ZCMD,'.')
  IF (&OPTN = 1,2,3,4)
     VER(&UID, NONBLANK)
     VER(&UID, LEN, '=', 7)
     &NR = TRUNC(&UID,3)
     VER(&NR, PICT, AAA)
  IF (&OPTN = 1)
     VER(&USNAME,NONBLANK, MSG = ICHM413A)
     VER(&USNAME,LEN, '<=', 21)
     VER(&UPSW, NONBLANK)
     &QNAME = TRUNC(&USNAME,1) /* get first char        @ZA85750*/
     IF (&QNAME = '''')        /* is first char a quote @ZA85750*/
       .MSG = ICHM413A         /* issue message         @ZA85750*/
       .CURSOR = USNAME        /* point to name         @ZA85750*/
  &VALCHK = ICHC41             /* check for end quote   @ZA91987*/
  IF (&OPTN = 2)
     VER(&UPSW, NONBLANK)
  VPUT (UID NAME UPSW) PROFILE
  &ZSEL = TRANS( &ZQ
   1,'CMD(%TSOUADD &UID:&USNAME:&UPSW)'
   2,'CMD(%TSOURES &UID:&USNAME:&UPSW)'
   3,'CMD(%TSOUDEL &UID)'
   4,'CMD(%TSOULST &UID)'
   5,'PANEL(RACFP05)'
   C,'PGM(isfisp) parm(H)'
   ' ',' '
   X,'exit'
   *,'?' )
  &ZTRAIL = .TRAIL
)END
./ ADD NAME=ISP@AUT  0101-01016-01016-1416-00087-00087-00000-RACFPNL
)ATTR
¢ type(text) attn(on)
^ area(dynamic) extend(on) scroll(on)
$ type(dataout) intens(high)
@ type(dataout) intens(low)
)BODY
%--- &ZSYSID --------------- MASTER APPLICATION MENU --------------- &ZSYSID -%
%OPTION ===>_ZCMD                                     +SCROLL ===>_PSCR%
%                                                                      %
%                                                  +USERID - &ZUSER    %
%                                                  +TIME   - &ZTIME    %
+
+ ^DYNAREA                                                           ^ +
+                                                                      +
+ Use UP and DOWN PF Keys or commands to scroll MENU.                  +
+ Enter END command to terminate.                                      +
)INIT
  .HELP    = ISP00005     /* Help for this master menu  CBIPO      */
  &ZPRIM   = YES          /* This is a primary option menu         */
&LINLEN = 68         /* Length of dynamic area lines       */
/* Normal primary option menu initialization               */
  &ZHTOP = ISR00003    /* Tutorial table of contents         */
  &ZHINDEX = ISR91000  /* Tutorial Index - 1st page          */
  VPUT (ZHTOP,ZHINDEX) PROFILE
/* Define data to be scrolled from products ordered.         */
/* All lines must be same length as the dynamic area width   */
/* in the panel body.                                        */
/* Define data to be scrolled                                */
/* All lines must be same length as the dynamic area width   */
/* in the panel body.                                        */
&MENU = '+
$AL @ALLOC   @- Allocate PDS or PS                                  +
$C5 @IOA 500 @- Integrated Operations Architecture (IOA)            +
$C  @IOA     @- Integrated Operations Architecture (IOA)            +
$CT @IOA     @- Integrated Operations Architecture (IOA CTT)        +
$HS @DFHSM   @- DFHSM recovery utilities                            +
$P  @PDF     @- ISPF/Program Development Facility                   +
$RM @RMF     @- Resource Measurement Facility                       +
$S  @DFSORT  @- Data Facility Sort                                  +
$SD @SDSF    @- System Display and Search Facility                  +
$SM @SMP/E   @- SMP/E Dialogs                                       +
$V  @VPS     @- Control VPS 6.2 Printers                            +
$X  @EXIT    @- Terminate ISPF using list/log defaults              '
/* End of scrollable data                     */
IF (&CUTP = ' ') &CUTP = 0   /* Initialize to display top of menu */
IF (&PSCR = ' ') &PSCR = PAGE  /* Initialize scroll amount        */
&JUNK = TRUNC(&MENU,&CUTP)     /* Truncate menu at cutoff point   */
&DYNAREA = .TRAIL              /* Portion of menu to be displayed */
)PROC
  &LASTLN = LVLINE(DYNAREA)    /* Last visable line of dynamic area*/
  IF (&ZCMD ^= ' ')            /* Make sure ZCMD value does not    */
    &ZQ = TRUNC(&ZCMD,'.')     /*  begin with a period             */
    IF (&ZQ = ' ')
      .MSG = ISPD241
  &ZSEL = TRANS( TRUNC (&ZCMD,'.')
            /**** ALLOCATE ****************************/
                AL,'PANEL(ALLOCDS)'
            /**** IOA *********************************/
                C,'cmd(%cttispfh)'
            /**** IOA *********************************/
               C5,'CMD(%IOAXTSO)'
            /**** DFHSM *******************************/
                HS,'PANEL(HSMUTIL)'
            /***** PDF Primary Option Menu ************/
                P,'PANEL(ISR@PRIM) NEWAPPL(PDF)'
            /**** RMF *********************************/
               RM,'PANEL(RMFPANEL)'
            /**** CTT *********************************/
               CT,'CMD(CTT)'
            /**** DFSORT ******************************/
                S,'PGM(ICEPMD01) PARM(&ZCMD) NEWAPPL(ICE) NOCHECK'
            /**** VPS *********************************/
            V,'PGM(VM62ISPF) PARM(&VMCOPT) NEWAPPL(VMCF) NOCHECK'
            /**** SDSF ********************************/
                SD,'PANEL(ZSDSFOP2) NEWAPPL(ISF)'
            /***** SMP/E ******************************/
                SM,'PGM(GIMSTART) PARM(&ZCMD) NOCHECK NEWAPPL(SMPE)'
            /**** JES2 ********************************/
                J,'PANEL(J2OFON)'
            /**** END OF PRODUCT LIST *****************/
                X,'EXIT'
                '  ','CMD(%SCROLL)'
                *,'?' )
            /**** SDSF ********************************/
                IF (&ZCMD = 'SD')
                   &ZSEL = 'PGM(ISFISP) NOCHECK NEWAPPL(ISF)'
)END
