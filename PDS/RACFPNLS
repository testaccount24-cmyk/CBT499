./ ADD NAME=ISPTERM  0101-01016-01016-1416-00050-00050-00000-RACFPNL
)ATTR
¢ TYPE(&ZF3ATTR)
)BODY
%----------------------------  ISPF DIALOG ERROR  ------------------------------
%COMMAND ===>_ZCMD                                                             +
%
%
%******************************************************************************
%*+                                                                          %*
%*+ %&ZERRSM                 +                                               %*
%*+ %&ZERRLM                                                                 %*
%*+                                                                          %*
%*+                                                                          %*
%*+ &ZDESCR                                                                  %*
%*+%&CSELINE                                                                 %*
%*+                                                                          %*
%*+                                                                          %*
%*+ Enter%HELP+command for further information regarding this error.         %*
%*+ Press%ENTER+key to terminate the dialog.                                 %*
%*+                                                                          %*
%*+ &ZF1                                                                     %*
%*+                         %&ZF2¢ZF3+&ZF4                                   %*
%*+                                                                          %*
%******************************************************************************
)INIT
  &ZCMD = ' '
  &ZF3ATTR = 'OUTPUT'
  .HELP = ISP94000
  &ZDESCR = TRANS(&CSEDSC ' ',' '
           P,'Panel line where error was detected:'
           C,'Current dialog statement:'
           S,'File tailoring input line:')
  &ZF1 = ' '  &ZF2 = ' '  &ZF3 = ' ' &ZF4 = ' ' &ZF3ATTR = 'OUTPUT'
  IF(&CSETEST = 'Y')
    .CURSOR = ZF3
    &ZF1 = 'OVERRIDE TERMINATION AND ATTEMPT TO CONTINUE'
    &ZF2 = '===>'
    &ZF3= 'NO'
    &ZF4 = '(YES or NO)'
    &ZF3ATTR = 'INPUT'
)PROC
  IF (.RESP = ENTER)
    IF (&ZCMD ^= ' ')
      .MSG = ISPZ001                      /* INVALID COMMAND            */
  &ZF3 = TRUNC(&ZF3,1)
  VER (&ZF3,LIST,Y,N)
  IF (&ZF3 ^= Y) .RESP = ENTER
  IF (&ZF3  = Y) .RESP = END
)END
